					},
					"country": {
						"type": "string"
					},
					"originalDocument": {
						"type": "string"
					},
					"customDocumentCode": {
						"type": "string"
					},
					"extractedData": {
						"$ref": "#/components/schemas/ExtractedData"
					}
				}
