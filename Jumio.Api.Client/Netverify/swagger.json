								"$ref": "#/components/schemas/DocDocumentDataOnly"
							},
							"transaction": {
								"$ref": "#/components/schemas/DocTransactionDataOnly"
							}
						}
					}
				]
			},
			"DocDocumentDataOnly": {
				"type": "object",
				"required": [
					"status"
				],
				"properties": {
					"status": {
						"type": "string",
						"enum": [
							"UNDEFINED",
							"UPLOADED",
							"EXTRACTED",
							"DISCARDED"
						]
					},
					"type": {
						"type": "string",
						"enum": [
							"UNDEFINED",
							"CC",
							"BS",
							"IC",
							"UB",
							"CAAP",
							"CRC",
							"CCS",
							"LAG",
							"LOAP",
							"MOAP",
							"TR",
							"VT",
							"VC",
							"STUC",
							"HCC",
							"CB",
							"SENC",
							"MEDC",
							"BC",
							"WWCC",
							"SS",
